#!/bin/bash
mkdir exec
for file in genChainExpStretch gen2MeshWeightedExpStretch genChainUniformStretch gen2MeshWeightedUniformStretch genFixedLengthUnweightedExpStretch genFixedLengthWeightedExpStretch gen2MeshUnweightedExpStretch genFixedLengthWeightedUniformStretch genFixedLengthUnweightedUniformStretch gen2MeshUnweightedUniformStretch genRandomWeightedExpStretch genRandomUnweightedExpStretch gen3MeshUnweightedExpStretch genRandomUnweightedUniformStretch genRandomWeightedUniformStretch gen3MeshUnweightedUniformStretch gen3MeshWeightedExpStretch gen3MeshWeightedUniformStretch
do
	g++ source/$file.cpp -o exec/$file -O3 --std=c++11
done
